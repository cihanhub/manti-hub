            function getNearestPlayer()
                local Closet
    
                for _,Player in pairs(Players:GetChildren()) do
                    if Player.Character:FindFirstChild("HumanoidRootPart") and Player.Name ~= game.Players.LocalPlayer.Name then
                        local TargetCharacter = Player.Character
                        local Distance = Player:DistanceFromCharacter(Hat.Position)
    
                        if Distance > 20 or (Closet and Distance >= Closet)  then
                            --Pass
                        else
                            return Player
                        end
                    end
                end
            end
