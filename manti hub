one()s.Parent=g;d.Animate.Disabled=true elseif e.RigType==Enum.HumanoidRigType.R15 then g.HumanoidRootPart.CFrame=d.UpperTorso.CFrame;d.Animate.Disabled=true;R15(d.UpperTorso,g['Torso'],Vector3.new(0,0.045,0))R15(d.LowerTorso,g['Torso'],Vector3.new(0,0.95,0))R15(d.LeftUpperArm,g['Left Arm'],Vector3.new(0,-0.242,0))R15(d.LeftLowerArm,g['Left Arm'],Vector3.new(0,0.35,0))R15(d.LeftHand,g['Left Arm'],Vector3.new(0,0.9,0))R15(d.RightUpperArm,g['Right Arm'],Vector3.new(0,-0.242,0))R15(d.RightLowerArm,g['Right Arm'],Vector3.new(0,0.35,0))R15(d.RightHand,g['Right Arm'],Vector3.new(0,0.9,0))R15(d.LeftUpperLeg,g['Left Leg'],Vector3.new(0,-0.60,0))R15(d.LeftLowerLeg,g['Left Leg'],Vector3.new(0,0.15,0))R15(d.LeftFoot,g['Left Leg'],Vector3.new(0,0.75,0))R15(d.RightUpperLeg,g['Right Leg'],Vector3.new(0,-0.60,0))R15(d.RightLowerLeg,g['Right Leg'],Vector3.new(0,0.15,0))R15(d.RightFoot,g['Right Leg'],Vector3.new(0,0.75,0))R15(d.HumanoidRootPart,g['HumanoidRootPart'])end;e.Died:Connect(function()pcall(function()g:BreakJoints()end)end)workspace.Camera.CameraSubject=g:FindFirstChild("Humanoid")local u=Instance.new("Sound",game:GetService("Players").LocalPlayer.Character.HumanoidRootPart)u.SoundId="rbxassetid://232351328"u.Looped=true;u.Playing=true;u.Pitch=1;wait()animid="rbxassetid://6495653138"plr=game.Players.LocalPlayer;bruhmomentrighthahahahhahah=plr.Character.Dummy;cf=bruhmomentrighthahahahhahah.HumanoidRootPart.CFrame;t=bruhmomentrighthahahahhahah.Torso;rs=t["Right Shoulder"]ls=t["Left Shoulder"]rh=t["Right Hip"]lh=t["Left Hip"]n=t["Neck"]rj=bruhmomentrighthahahahhahah.HumanoidRootPart["RootJoint"]rsc0=rs.C0;lsc0=ls.C0;rhc0=rh.C0;lhc0=lh.C0;rjc0=rj.C0;nc0=n.C0;gc0=CFrame.new()orsc0=rs.C0;olsc0=ls.C0;orhc0=rh.C0;olhc0=lh.C0;orjc0=rj.C0;onc0=n.C0;count2=100;maxcount2=100;game["Run Service"].Heartbeat:Connect(function()count2=count2+1;if count2<=maxcount2 then rs.Transform=rs.Transform:Lerp(rsc0,count2/maxcount2)ls.Transform=ls.Transform:Lerp(lsc0,count2/maxcount2)rh.Transform=rh.T