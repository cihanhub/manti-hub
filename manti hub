"],Vector3.new(0,-0.4,-1.7),Vector3.new(0,115,0))local s=c:FindFirstChild("Daniel").Handle;s.AccessoryWeld:Destroy()Align(s,f["Head"],Vector3.new(0,-0.3,0.1),Vector3.new(0,180,0))f.Animate:Destroy()local t=c.Animate:Clone()t.Parent=f;c.Animate.Disabled=true end;d.Died:Connect(function()pcall(function()Noclip:Disconnect()Netless:Disconnect()Test:Disconnect()f:BreakJoints()end)end)workspace.Camera.CameraSubject=f:FindFirstChild("Humanoid")
