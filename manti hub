local Window = Library:CreateWindow('Mantı Hub', 'Dollhouse RP', 'Mantı Hub', 'rbxassetid://6862780932', false, 'VisualUIConfigs', 'Default')

local Tab = Window:CreateTab('FE Animations', true, 'rbxassetid://3926305904', Vector2.new(484, 44), Vector2.new(36, 36))

local Section = Tab:CreateSection('FE Animations')

