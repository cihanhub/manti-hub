    Align(Character["Right Arm"], Dummy["Right Arm"])
    Align(Character["Left Arm"], Dummy["Left Arm"])
    Align(Character["Right Leg"], Dummy["Right Leg"])
    Align(Character["Left Leg"], Dummy["Left Leg"])
    Align(Character["Right Arm"], Dummy["Right Arm"])
    Humanoid:ChangeState("Physics")
    local Hat = Character:FindFirstChild("Pal Hair").Handle
    Hat.AccessoryWeld:Destroy()
    Hat.Mesh:Destroy()
    Align(Hat, Dummy["Right Arm"], Vector3.new(0, 0, 0), Vector3.new(90, 0, 0))
    Character["Right Arm"].Name = "Fling"
    Dummy.Animate:Destroy()
    local Animate = Character.Animate:Clone()
