                FlingPart.Position = (getNearestPlayer().Character.HumanoidRootPart.Position) or  LocalCharacter["MARTILLO"].Handle.Position
            else
                FlingPart.Position = LocalCharacter["MARTILLO"].Handle.Position
            end
        end
        if FlingEXEvent == true then
            FlingPart.Position = Mouse.Hit.p
    --		Hat2.Position = Mouse.Hit.p
        end
    end)
